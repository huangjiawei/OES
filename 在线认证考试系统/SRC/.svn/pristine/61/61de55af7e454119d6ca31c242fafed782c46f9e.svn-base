@foreach (var i in Model.Questions)
{
    <div id="@i.QuestionNum">
        <p>大题号：@i.BigNum 题目类型：@i.QType.ToString()</p>
        <p>
            @i.QuestionNum
            :
            @i.Question
        </p>
        @if (i.Options != null && i.Options.Count > 0)
        {
            <hr />
            foreach (var o in i.Options)
            {
                <p>
                    @o.Op
                    :
                    @o.Content
                </p>

            }

        }
        <hr />
        <p>@i.Info</p>
        <hr />
        <p>@i.Answer</p>

    </div>
}
